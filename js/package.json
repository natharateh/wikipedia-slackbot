{
  "name": "wikipedia-slackbot",
  "version": "1.0.0",
  "description": "Wikipedia Slackbot will help you search the sum of all human knowledge, without leaving Slack.",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel lib -d dist",
    "lint": "eslint --cache --max-warnings 0 lib/*.js",
    "start": "npm run build && node dist/index.js",
    "precommit": "npm run lint",
    "test": "./node_modules/.bin/mocha test/*.js --exit"
  },
  "keywords": [
    "wikipedia",
    "slack",
    "bot",
    "slackbot"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "eslint": "^5.2.0",
    "express": "^4.17.3",
    "husky": "^1.3.1",
    "url-parse": "^1.4.3",
    "mocha": "^5.2.0",
    "redis": "^2.8.0",
    "request-promise-native": "^1.0.5"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "request": "^2.87.0"
  }
}
